        "clsx": "^1.1.1",
        "file-saver": "^2.0.2",
        "history": "^4.10.1",
        "keycloak-js": "^7.0.1",
        "lodash": "^4.17.20",
        "material-table": "^1.60.0",
        "material-ui-dropzone": "^3.2.1",
        "md5": "^2.2.1",
        "moment": "^2.26.0",
        "node-sass": "^4.14.1",
        "prop-types": "^15.7.2",
        "react": "^16.14.0",
        "react-big-calendar": "^0.28.1",
        "react-bootstrap": "^1.0.1",
        "react-contextmenu": "^2.14.0",
