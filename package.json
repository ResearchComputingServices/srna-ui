        "uuid": "^8.1.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "deploy": "docker-compose up --build -d",
        "test": "react-scripts test --watchAll=false",
        "test:dev": "react-scripts test",
        "test:coverage": "react-scripts test --watchAll=false --coverage",
        "test:report": "open ./coverage/lcov-report/index.html",
        "mock:api": "json-server --watch mockApi/db.json --port 4000",
        "eject": "react-scripts eject",
        "lint": "node_modules/eslint/bin/eslint.js 'src/**/*.js'",
        "lint:fix": "node_modules/eslint/bin/eslint.js 'src/**/*.js' --fix",
        "serve": "npm run build && serve -s build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/react": "^11.1.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-react-hooks": "^2.5.1",
        "eslint-plugin-standard": "^4.0.1"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!src/index.js",
            "!src/services/**"
        ]
    }
}
